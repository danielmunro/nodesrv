{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-sonarts"
  ],
  "rules": {
    "semicolon": [true, "never"],
    "no-console": false,
    "no-empty": {
      "severity": "warning"
    },
    "interface-name": [true, "never-prefix"],
    "no-identical-functions": false
  }
}