{
  "type": "sqlite",
  "database": "database.db",
  "synchronize": false,
  "logging": false,
  "entities": [
    "src/room/model/*.ts",
    "src/mob/model/*.ts",
    "src/player/model/*.ts",
    "src/attributes/model/*.ts",
    "src/item/model/*.ts"
  ],
  "migrations": [
    "src/migration/**/*.ts"
  ],
  "subscribers": [
    "src/subscriber/**/*.ts"
  ]
}